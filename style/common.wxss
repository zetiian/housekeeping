/* color */
 .main-font-color{
   color: #F7B500;
 }


.app-main-bg {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 418rpx;
    z-index: -1;
    background: linear-gradient(180deg, rgba(29, 127, 201, 1) 0%, rgba(29, 127, 201, 0) 100%);
  }
  .app-content {
    padding-top: 60rpx;
  }
  .app-content.ipx {
    padding-top: 100rpx;
  }
  .app-tab-content {
    padding-top: 150rpx;
  }
  .app-tab-content.ipx {
    padding-top: 200rpx;
  }
  .app-bottom{
    height: 50rpx;
  }
  .app-contanier {
    width: 686rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(107, 114, 123, 0.24);
    border-radius: 12rpx;
    margin-left: 32rpx;
  }
  /* flex S */
  .app-flex {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
  }
  .app-flex-center{
    align-items: center;
  }
  .app-flex-row-center{
    justify-content: center;
  }
  .app-flex-column {
    flex-direction: column;
  }
  
  .app-flex-around {
    justify-content: space-around;
  }
  .app-flex-end{
    align-items: flex-end;
  }
  .app-flex-between {
    justify-content: space-between;
  }
  /* flex E */
  
  /* swiper S */
  .app-swiper {
    height: 502rpx;
    margin: 0 32rpx;
  }
  .app-swiper-item {
    width: 686rpx;
    height: 192rpx;
    box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(107, 114, 123, 0.24);
    border-radius: 20rpx;
    overflow: hidden;
  }
  
  /* swiper E */
  
  .app-is-fixed {
    position: fixed;
  }
  .app-link{
    color: #F7B500;
  }
  /* app button */
  .app-button {
    width: 160rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: linear-gradient(90deg, rgba(252, 219, 0, 1) 0%, rgba(247, 181, 0, 1) 100%);
    border-radius: 32rpx;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
  }
  /* app icon */
  .app-arrow-right{
    width:30rpx;
    height:30rpx;
    background-position: center;
    background-size: contain;
    background-repeat: no-repeat;
    background-image:url(data:image/png;base64,)
  }
  .app-arrow-down{
    width:30rpx;
    height:30rpx;
    background-position: center;
    background-size: contain;
    background-repeat: no-repeat;
    background-image:url(data:image/png;base64,)
  }
  .app-arrow-up{
    width:30rpx;
    height:30rpx;
    background-position: center;
    background-size: contain;
    background-repeat: no-repeat;
    background-image:url(data:image/png;base64,)
  }
  
  .app-choose-icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 16rpx;
    border: 2rpx solid #747A86;
    border-radius: 18rpx;
  }
  .app-choose-icon.active {
    border-color: #1D7FC9;
    background: url(data:image/png;base64,) no-repeat center center/ 100% 100%;
  }