.my-account {
  justify-content: flex-start;
  align-items: flex-start;
  padding: 20rpx;
  margin-top: 32rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #727484;
}

.my-account-box {
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
  justify-content: space-around;
}

.question-item {
  height: 108rpx;
  align-items: center;
  border-bottom: 1rpx solid #AFB9C6;
}


.question-item-name {
  margin-left: 30rpx;

}
.my-account-btn{
  width: 100%;
  justify-content: center;
}
.app-contanier{
  box-shadow: none;
}
.choose-icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 16rpx;
    opacity: 0;
    border-radius: none;
    background: url(data:image/png;base64,) no-repeat center center/ 100% 100%;
}
.choose-icon.active {
  opacity: 1;

}