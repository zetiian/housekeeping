.container{
    margin-top: 10vh;
}

.img-container{
    position: relative;
    margin-left: 32rpx;
    margin-bottom: 40rpx;
    width: 686rpx;
    height: 276rpx;
    color: #727484;
    font-size: 48rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    border-radius: 16rpx;
    background-image: url(data:image/png;base64,);
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
}
.img-container.service{
    background-image: url(data:image/png;base64,);
}
.title{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 50rpx;
}
.register-btn{
    position: absolute;
    width: 686rpx;
    height: 276rpx;
    top: 0;
    left: 0;
    opacity: 0;
    z-index: 100;
    
}
