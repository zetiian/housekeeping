page{
  overflow-y: scroll!important;
}
.my-card{
  height: 324px;
  justify-content: center;
  box-shadow: 0px 0px 6px 0px rgba(123, 115, 107, 0.24);
}
.my-card-1{
  align-items: center;
  height: 236px;
  justify-content: center;
  box-shadow: 0px 0px 6px 0px rgba(123, 115, 107, 0.24);
}
.app-btn{
  width: 460rpx;
}

.my-warning{
  margin-top: 40rpx;
}
.my-avatar{
  width: 228rpx;
  height: 228rpx;
  border-radius: 128rpx;
  overflow: hidden;
}
.my-nickname{
  font-size:36rpx;
  font-weight:500;
  color:rgba(53,52,52,0.85);
  margin: 30rpx 0 20rpx;
}
.my-account{
  height: 192rpx;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 20rpx;
  margin-top: 32rpx;
}
.my-account-box{
  width: 100%;
  margin-top: 20rpx;
}
.my-account-money{
  justify-content: flex-start;
  align-items: flex-end;
}
.my-account-num{
  color: #0C324E;
  font-size: 48rpx;
  font-weight: 500;
  line-height: 48rpx;
}
.my-ticket{
  justify-content: flex-start;
  align-items: flex-start;
  padding: 0 20rpx;
  margin-top: 32rpx;
}
.my-ticket-item{
  height: 100rpx;
  width: 100%;
  font-size:32rpx;
  font-weight:400;
  color:rgba(107,114,123,1);
  border-bottom: 1px solid #eee;
  align-items: center;
  position: relative;
}
.my-ticket button{
  position: absolute;
  top: 0;
  left: 0;
  width: 668rpx;
  height: 100%;
  opacity: 0;
}
.app-arrow-right{
  margin-bottom: -2rpx;
  margin-left: 10rpx;
}
.my-ticket-item:nth-last-of-type(1){
  /* border-bottom: none; */
}
.my-ticket-left-icon{
  width: 38rpx;
  height: 38rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  margin-right: 30rpx;
}
.my-ticket-left-icon.contact{
  background-image: url(data:image/png;base64,);
}
.my-ticket-left-icon.question-feedback{
  background-image: url(data:image/png;base64,);
}
.my-ticket-left-icon.my-address{
  background-image: url(data:image/png;base64,);
}
.my-ticket-left-icon.about-us{
  background-image: url(data:image/png;base64,);
}
.my-item {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #727484;
  margin-bottom: 16rpx;
  height: 196px ;
  margin: 8px 0;
  padding:16px;
  box-shadow: 0px 0px 6px 0px rgba(123, 115, 107, 0.24);
}

.sercice-data{
  width: 50%;
  font-size: 40rpx;
  height: 120rpx;
  justify-content: space-around;
  align-items: center;
  margin-top: 70rpx;

}
.sercice-data:nth-last-of-type(1){
  border-left: 1rpx solid #F0EDE3;
}
.tag{
  margin-top: -130rpx;
}
.tag .base-button{
  background-color: #fff!important;
}
.order-box {
  padding-bottom: 300rpx!important;
}