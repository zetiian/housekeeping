
.my-card{
  height: 324px;
  justify-content: center;
  box-shadow: 0px 0px 6px 0px rgba(123, 115, 107, 0.24);
}
.app-btn{
  width: 460rpx;
  
}
.btn-text{
  font-size: 40rpx;
}
.my-warning{
  margin-top: 40rpx;
}
.my-avatar{
  width: 128rpx;
  height: 128rpx;
  border-radius: 64rpx;
  overflow: hidden;
}
.my-nickname{
  font-size:36rpx;
  font-weight:500;
  color:rgba(53,52,52,0.85);
  margin: 16rpx 0;
}
.my-account{
  height: 192rpx;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 20rpx;
  margin-top: 32rpx;
}
.my-account-box{
  width: 100%;
  margin-top: 20rpx;
}
.my-account-money{
  justify-content: flex-start;
  align-items: flex-end;
}
.my-account-num{
  color: #0C324E;
  font-size: 48rpx;
  font-weight: 500;
  line-height: 48rpx;
}
.my-ticket{
  justify-content: flex-start;
  align-items: flex-start;
  padding: 0 20rpx;
  margin-top: 32rpx;
}
.my-ticket-item{
  height: 100rpx;
  width: 100%;
  font-size:32rpx;
  font-weight:400;
  color:rgba(107,114,123,1);
  border-bottom: 1px solid #eee;
  align-items: center;
  position: relative;
}
.my-ticket button{
  position: absolute;
  top: 0;
  left: 0;
  width: 668rpx;
  height: 100%;
  opacity: 0;
}
.app-arrow-right{
  margin-bottom: -2rpx;
  margin-left: 10rpx;
}
.my-ticket-item:nth-last-of-type(1){
  border-bottom: none;
}
.my-ticket-left-icon{
  width: 38rpx;
  height: 38rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  margin-right: 30rpx;
}
.my-ticket-left-icon.contact{
  background-image: url(data:image/png;base64,);
}
.my-ticket-left-icon.question-feedback{
  background-image: url(data:image/png;base64,);
}
.my-ticket-left-icon.about-us{
  background-image: url(data:image/png;base64,);
}