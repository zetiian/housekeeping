.app-arrow-right{
  margin-left: 10rpx;
  margin-bottom: -4rpx;
}
.app-one-line {
  position: absolute;
  left: 80rpx;
  width: 590rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: flex-start;
  
}
.notice-swiper{
  height: 80rpx;
  width: 500rpx;
  margin-left: -5rpx;

}
.notice-swiper-item{
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
}


