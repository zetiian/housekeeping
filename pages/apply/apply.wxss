.order-box {
    padding-bottom: 150rpx!important;
}
.apply-avatar{
    height: 235px;
    box-shadow: 0px 0px 6px 0px rgba(123, 115, 107, 0.24);
    font-size: 36rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #727484;
    justify-content: center;
    background-color: #fff;
}
.user-avatar{
    margin-bottom: 20rpx;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    overflow: hidden;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    background-image: url(data:image/png;base64,);
}
.van-field{
    height: 120rpx;
    
}
.van-cell{
    border-bottom: 1rpx solid #eee;
    font-size: 32rpx;
    color: #727484;
}
.id-card-special{
    background-image: none;
    position: relative;
    padding:40rpx 0 20rpx;
    align-items: flex-start;
    margin-left: 30rpx;
  }
.id-card-special-plus{
    width: 300rpx;
    height: 200rpx; 
    background: #fff;
    border-radius: 8rpx;
    border: 1px solid #AFB9C6;
    background-position: center;
    background-repeat: no-repeat;
    background-size: contain;
    background-image: url(data:image/png;base64,);
}
.upload-3{
    position: absolute;
    top: 0;
    left: 0;
  }
