.foot-fix {
  position: fixed;
  z-index: 15;
  bottom: 0;
  left: 0;
  right: 0;
  opacity: 1;
  transform-style: preserve-3d;
  transform: translate3d(0,0,0);
}
.foot-fix.hide {
  visibility: hidden;
  opacity: 0;
}
.foot-fix.ani {
  transition: all 0.4s ease-in-out;
}
.foot-nav {
  height: 98rpx;
  width: 750rpx;
  background-color: #fff;
  z-index: 10;
  display: flex;
  display: -webkit-flex;
  align-items: stretch;
  box-shadow: 0px -8px 8px 0px rgba(23, 26, 37, 0.03);
}
.foot-nav__item {
  font-size: 22rpx;
  color: #1c1c1c;
  font-weight: bold;
  text-align: center;
  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 1 1 auto;
  line-height: 1.4;
  padding: 0;
  border: 0;
  border-radius: 0;
  background-color: transparent;
  overflow: visible;
  position: relative;
}
.foot-nav__item::after {
  border: 0;
}
.foot-nav__icon {
  width: 44rpx;
  height: 44rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  margin-top: 20rpx;
  flex: 0 0 auto;
  color: #747a86;
}
.foot-nav__icon-road {
  background-image: url(data:image/png;base64,);
}
.foot-nav__icon-order {
  background-image: url(data:image/png;base64,);
}

.foot-nav__icon-mine {
  background-image: url(data:image/png;base64,);
}


.foot-nav__icon-text {
  margin-top: 4rpx;
  margin-bottom: 14rpx;
  color: #747a86;
}
.foot-nav__item-active .foot-nav__icon-text {
  color: #eec747;
}
.foot-nav__item-active .foot-nav__icon-road {
  background-image: url(data:image/png;base64,);
}
.foot-nav__item-active .foot-nav__icon-order {
  background-image: url(data:image/png;base64,);
}

.foot-nav__item-active .foot-nav__icon-mine {
  background-image: url(data:image/png;base64,);
}

.foot-nav-ipx-hr {
  height: 68rpx;
  background: #fff;
}
.foot-nav__icon-text-publish {
  margin-top: 6rpx;
}

.mask {
  position: absolute;
  z-index: 20;
  right: 0;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.6);
  height: 0;
  opacity: 0;
  transition: opacity 0.3s ease-in-out, height 0.1s ease-in-out 0.3s;
}
.mask.show {
  transition: opacity 0.3s ease-in-out;
  height: 100%;
  opacity: 1;
}
.redpoint {
  width: 6px;
  height: 6px;
  background-color: red;
  border-radius: 100%;
  position: absolute;
  top: 12rpx;
  right: 32rpx;
}
.getUserInfo{
  width: 100%;
  height: 98rpx;
  background-color: #0a0;
}